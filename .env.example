# Variables d'environnement pour l'application CARS MOTION

# Configuration de Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# URL de connexion PostgreSQL pour la base de données Supabase
DATABASE_URL=postgres://postgres:password@your-project.supabase.co:6543/postgres

# Variables pour le frontend (doivent être préfixées par VITE_)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Autres paramètres
SESSION_SECRET=your-session-secret